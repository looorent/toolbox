{
  "name": "toolbox",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "biome check .",
    "lint:fix": "biome check --fix .",
    "build-only": "vite build",
    "build": "pnpm run type-check && pnpm run build-only",
    "preview": "pnpm run build && wrangler dev",
